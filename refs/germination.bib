@article{Rudolf:2019aa,
	annote = {https://doi.org/10.1111/ele.13277},
	author = {Rudolf, Volker H. W.},
	booktitle = {Ecology Letters},
	da = {2019/08/01},
	date = {2019/08/01},
	date-added = {2020-12-17 14:10:01 -0500},
