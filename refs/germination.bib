@article{Baskin:1988aa,
	annote = {https://doi.org/10.1002/j.1537-2197.1988.tb13441.x},
	author = {Baskin, Carol C. and Baskin, Jerry M.},
	booktitle = {American Journal of Botany},
	da = {1988/02/01},
